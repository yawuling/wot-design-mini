<demo-block>
  <wd-button bind:click="openCalendar">打开日历</wd-button>
  <view>{{ formatValue }}</view>
</demo-block>
<calendar
  show="{{ show }}"
  data-type="{{ dataType }}"
  date-type="{{ dateType }}"
  value="{{ value }}"
  default-time="{{ ['00:00:00', '23:59:59'] }}"
  bind:confirm="handleConfirm"
/>